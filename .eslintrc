{
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 7,
      },
      "max-len": [2, {
          "code": 90,
          "tabWidth": 2,
          "ignoreUrls": true,
          "ignoreComments": true,
          "ignoreStrings": true,
          "ignorePattern": "'.*': '.*',",
        }],
    "extends": "eslint-config-google",
     "rules": {
        "no-undef": 1
    },
    "globals": {
        "Promise": true
    }
}
